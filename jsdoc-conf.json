{
  "source": {
    "include": [ "lib/", "lib/formatters/" ]
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "destination": "docs/",
    "readme": "README.md",
    "template": "node_modules/ink-docstrap/template",
    "tutorials": "pages/"
  },
  "templates": {
    "github": "messageformat/messageformat.js",
    "logoFile": "logo/messageformat.svg",
    "outputSourceFiles": true,
    "search": false,
    "systemName": "messageformat",
    "theme": "flatly"
  }
}
